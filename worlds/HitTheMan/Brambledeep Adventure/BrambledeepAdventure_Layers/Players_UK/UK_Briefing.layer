   "UK"
  }
  m_iOrder 2
 }
 BLU_Briefing_3_Execution2 {
  coords 4457.333 202.057 7705.504
  m_sTitle "III. Execution:"
  m_sTextData "<h2>III: Execution</h2>"\
  "<b>Intent:</b>"\
  "Defend the Ammo Dumps and our foothold around Brambledeep Lake."\
  ""\
  "<b>1. Concept of the Operation:</b>"\
  "Company Commander's Discretion."\
  "It is adviced to the Platoon to setup killzones on likely attack routes, find and destroy the enemy FO and prepare fallback positions."\
  ""\
  "<b>2. Maneuver</b>"\
  "Company Commander's Discretion."\
  "It is adviced to NOT split up the sections too far so they can mutually support one another or be able to quickly support as needed."
  m_aVisibleForFactions {
   "UK"
  }
  m_iOrder 5
 }
