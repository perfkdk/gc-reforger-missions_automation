  m_sTitle "V. Command & Signals:"
  m_sTextData "<h2>V. Command & Signal:</h2>"\
  "<b>I. Chain of Command:</b>"\
  "21, 21A, 21B, etc."\
  ""\
  "<b>II. Callsigns:</b>"\
  "Platoon HQ ---------------------------- 21"\
  "- Platoon Leader -------------------- 21 Sunray"\
  "- Platoon Warrant ------------------ 21 Sunray Minor OR 21F - Foctor"\
  "1st Section --------------------------- 21A - Alpha"\
  "2nd Section -------------------------- 21B - Bravo"\
  "2nd Section -------------------------- 21C - Charlie"\
  ""\
  "<b>- - - Vehicle/LAV Callsigns when Inf dismounted:</b>"\
  "Z29 or Zulu 29"\
  "Zulu 21 Alpha"\
  "Zulu 21 Bravo"\
  "Etc."\
  ""\
  "<b>III. Frequencies:</b>"\
  "Global NATO Net ------- 50.00Mhz"\
  "1st Platoon Net -------- 47.00Mhz"
